@model IEnumerable<User>

@{
	ViewData["Title"] = "Users";
}

<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>Username</th>
			<th>Name</th>
			<th>Email</th>
			<th>Phone</th>
			<th>Address</th>
			<th>Company</th>
			<th>Website</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var user in Model)
		{
			<tr>
				<td><a asp-area="" asp-controller="Users" asp-action="Index", asp-route-id="@user.Id">@user.Id</a></td>
				<td>@user.Username</td>
				<td>@user.Name</td>
				<td>@user.Email</td>
				<td>@user.Phone</td>
				<td>@user.Address.Suite @user.Address.Street @user.Address.City @user.Address.Zipcode @user.Address.Geo</td>
				<td>@user.Company.Name @user.Company.Bs @user.Company.CatchPhrase</td>
				<td>@user.Website</td>
			</tr>
		}
	</tbody>
</table>
